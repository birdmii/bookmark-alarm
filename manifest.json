{
    "name": "Bookmarkalarm",
    "version": "1.1",
    "description": "Bookmarkalarm helps you set alarm to each bookmark to look at it at the proper time",
    "permissions": [
        "bookmarks"
    ],
    "browser_action": {
        "default_title": "Bookmarkalarm",
        "default_popup": "popup.html"
    },
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}